{{#if findedUser}}
    view my task list
    <button {{action "logout"}} class="btn btn-primary">Login OUT</button>
{{else}}
<div class="row">
  <div class="col-4">
    <div class="form-group">
      {{input type="text" class="form-control" name="user_name" value=user.name placeholder="Enter your name"}}
    </div>
    <div class="form-group">
      {{input type="password" class="form-control" name="user_password" value=user.password placeholder="Enter your password"}}
    </div>
    <button {{action "login"}} class="btn btn-primary">Login</button>
  </div>
  {{#if user_error_login.status}}
    <p>{{user_error_login.message}}</p>
  {{/if}}
</div>
{{/if}}
